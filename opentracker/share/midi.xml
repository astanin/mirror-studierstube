<!--DOCTYPE OpenTracker SYSTEM "opentracker.dtd"-->
<!--?xml-stylesheet type="text/xsl" href="E:\opentracker\data\ot2dot.xsl"?-->
<OpenTracker>
  <configuration>
	<ConsoleConfig headerline="Midi Input" interval="1"/>
  </configuration>

  <ConsoleSink comment="MidiSrc output" active="on">
    <FileSink file="midi.out">
     <MidiSource device="In-B USB MidiSport 2x2"/>
    </FileSink>
  </ConsoleSink>

</OpenTracker>
